<?xml version="1.0" encoding="UTF-8"?>
<api context="/testCall" name="TestAPI" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST" uri-template="/list">
        <inSequence>
            <log level="custom">
                <property name="STATUS" value="Invoking the TestAPI and responding back..."/>
            </log>
            <log level="full"/>
            <respond/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </resource>
</api>
